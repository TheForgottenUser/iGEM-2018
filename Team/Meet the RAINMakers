    <div class="team_slideshow">
      //javascript code for the slideshow
      <a href="javascript:gotoshow()"><img src="food1.jpg" name="slide" border=0 width=300 height=375></a>
      <script>
        //configure the paths of the images, plus corresponding target links
        slideshowimages("Sarah_Bortel.jpg","Kira Boyce")
        slideshowlinks("http://2018.igem.org/wiki/images/3/3b/T--Tacoma_RAINMakers--Sarah_Bortel.png","http://2018.igem.org/wiki/images/4/4e/T--Tacoma_RAINMakers--Kira_Boyce.png")

        //configure the speed of the slideshow, in milliseconds
        var slideshowspeed=2000

        var whichlink=0
        var whichimage=0
        function slideit(){
          if (!document.images)
          return
          document.images.slide.src=slideimages[whichimage].src
          whichlink=whichimage
          if (whichimage<slideimages.length-1)
          whichimage++
          else
          whichimage=0
          setTimeout("slideit()",slideshowspeed)
        }
        slideit()
      </script>
    </div>
    

